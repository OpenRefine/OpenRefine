<div class="fbs-flyout-content">
  <h1 id="fbs-flyout-title">$propertyUri</h1>
  <h2 class="fbs-flyout-subtitle">Sample Instances:</h2>
  <table style="width:100%; border:1px solid #ccc;border-collapse:collapse;">
    <tr>
      <th>Subject</th>
      <th>Object</th>
    </tr>
    #foreach($instance in $instances)
      <tr>
        #foreach($val in $instance)
        	<td style="border:1px solid #ccc;"><p>$val</p></td>
        #end
      </tr>
    #end
  </table>
</div>