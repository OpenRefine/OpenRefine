<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
 
<suite name="Refine Unit Tests">
  <test name="tests">
    <packages>
      <package name="com.google.refine.tests.*"/>
    </packages>
    <groups>
      <run>
        <exclude name="broken" />
      </run>
    </groups>
  </test>
</suite>
